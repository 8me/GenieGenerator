<?xml version="1.0" encoding="ISO-8859-1"?>

<alg_conf>
  <param_set name="Default">
    <!-- This parameter supports env var expansion, like
    $MY_LIB_DIR/my_lib.root -->
    <param type="string" name="EventLibraryPath"> Dummy.root </param>
    <!-- Should the library be read from disk on-demand ("true", recommended)
    or read completely into memory up-front ("false") -->
    <param type="bool" name="OnDemand"> true </param>
  </param_set>
</alg_conf>
