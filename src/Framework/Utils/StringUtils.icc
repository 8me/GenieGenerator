 template<class T>
    bool genie::utils::str::Convert( const vector<std::string> & input, std::vector<T> & v ) { 

  	v.resize( input ) ;
  	bool success = true ;

    for ( unsigned int i = 0 ; i < input.size() ; ++i ) {
      std:stringstream temp ;
      temp << input[i] ;
      v[i] << temp ;
      success = success && temp.good() ;
    }
	
	return success ;
	
}